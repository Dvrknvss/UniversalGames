--Keep this
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Universal Games/FE ScriptHub", "DarkTheme")
-----------------------------------------------------------------------------------------------------------------------------
--Tabs
local UFE = Window:NewTab("Universal FE")
local Games = Window:NewTab("Games")
local CreditsConfig = Window:NewTab("Credits / Config")
--Sections
local SectionUFE = UFE:NewSection("Script: ")
local SectionGames = Games:NewSection("Total of Supported Games: 2")
local SectionScripts = Games:NewSection("Total of Scripts: 2")
local SectionTeleport = Games:NewSection("Choose Supported Game to Teleport!")
local SectionCreditsConfig = CreditsConfig:NewSection("Credits/Config")
--Buttons
SectionUFE:NewButton("Univesal FE ScriptHub", "Main Hub", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Dvrknvss/UniversalFEScriptHub/main/Script'))()
end)
--Dropdowns
SectionTeleport:NewDropdown("Select Game to Teleport", "", {"Blox Fruits", "Da Hood"}, function(Value)
    _G.gamestoteleport = Value
end)
SectionTeleport:NewButton("Teleport To Selected Game", "", function()
    if _G.gamestoteleport == "Select Game to Teleport" then
        SolarisLib:Notification("Warning!", "Select game first to teleport!")
    elseif _G.gamestoteleport == "Blox Fruits" then
        game:GetService("TeleportService"):Teleport(2753915549, player)
    elseif _G.gamestoteleport == "Da Hood" then
        game:GetService("TeleportService"):Teleport(2788229376, player)
    end
end)
-- Supported Games
--Blox Fruits
if game.PlaceId == 2753915549 then
    local SectionSupportedGame = Games:NewSection("Detected Supported Game!")
    SectionSupportedGame:NewButton("HoHo Hub", "", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
    end)
end
-- Da Hood
if game.PlaceId == 2788229376 then
    local SectionSupportedGame = Games:NewSection("Detected Supported Game!")
    SectionSupportedGame:NewButton("Arctic", "", function()
        loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/PolarWasHere/Arctic/main/Arctic"))()
    end)
end
-- Pet Simulator X 
if game.PlaceId == 6284583030 then
    local SectionSupportedGame = Games:NewSection("Detected Supported Game!")
    SectionSupportedGame:NewButton("HoHo Hub", "", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
    end)
end
-- Zombie Attack
if game.PlaceId == 1240123653 then
    local SectionSupportedGame = Games:NewSection("Detected Supported Game!")
    SectionSupportedGame:NewButton("HoHo Hub", "", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
    end)
end
-- Tradelands
if game.PlaceId == 198116126 then
    local SectionSupportedGame = Games:NewSection("Detected Supported Game!")
    SectionSupportedGame:NewButton("HoHo Hub", "", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
    end)
end
